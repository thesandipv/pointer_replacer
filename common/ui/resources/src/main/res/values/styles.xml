<?xml version="1.0" encoding="utf-8"?>

<resources>

    <!--Toolbar-->
    <style name="Widget.MyTheme.Toolbar" parent="Widget.Material3.Toolbar.Surface">
        <item name="actionMenuTextColor">?attr/colorOnPrimarySurface</item>
    </style>

    <style name="SeekBarButtonStyle">
        <item name="android:tint">?attr/colorPrimary</item>
        <item name="android:background">?attr/selectableItemBackgroundBorderless</item>
    </style>

    <style name="MyStyle.FilterChip" parent="Widget.Material3.Chip.Filter" />

    <style name="MyStyle.ShapeAppearance.SmallComponents" parent="ShapeAppearance.Material3.SmallComponent">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">16dp</item>
    </style>

    <style name="MyStyle.ShapeAppearance.MediumComponents" parent="ShapeAppearance.Material3.MediumComponent">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">16dp</item>
    </style>

    <style name="TextAppearance.MyStyles.Title" parent="TextAppearance.FontUbuntu.Headline6">
        <item name="android:textColor">?attr/colorOnPrimarySurface</item>
    </style>
</resources>
